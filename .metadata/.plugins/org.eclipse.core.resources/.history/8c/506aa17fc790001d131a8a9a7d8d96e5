
public class Student {
	String name ;
	int assigmentsScore ;
	int examScore;
	char Grade;
	
	
	Student(){	
	
	}
	String toString() {
		double totalScore = assigmentsScore *0.4  + examScore * 0.6;
		if(totalScore >= 90) {
			Grade = 'A';
		} else if(totalScore >= 80){
			Grade = 'B';
		} else {
			Grade = 'C';
		}
		String i = "이름 : "+ "과제 점수 :" + "시험 점수 : " + "학점  : "
		return 
	}
	
	
	
	
}
