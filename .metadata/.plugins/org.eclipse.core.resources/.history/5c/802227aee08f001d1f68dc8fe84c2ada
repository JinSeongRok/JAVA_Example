
public class MultidimenstionalArrayByNewExample {

	public static void main(String[] args) {
		int[][] mathScores = new int[2][3];
		
		for (int i=0; i<mathScores.length; i++) {  // 반의 수 만큼 반복
			for (int k=0; k<mathScores[i].length; k++) {   // 해당반의 학생 수 만큼 반복
				System.out.println("mathScores[" + i + "][" + k + "]" + mathScores[i][k]);
			}
		}
		System.out.println();
		
		mathScores[0][0] = 80;
		
		mathScores[0][0] = 80;
		mathScores[0][1] = 83;
		mathScores[0][2] = 85;
		mathScores[1][0] = 86;
		mathScores[1][1] = 90;
		mathScores[1][2] = 92;
		
		
	}

}
