
public class IfDiceExample {

	public static void main(String[] args) {
		int num = ((int)Math.random()* 6)+1;
		int num2 = ((int)Math.random()* 6)+1;
		int sum = num + num2;
		
		System.out.println("첫번째 주사위 : " + num + " 두번째 주사위 : " + num2);
		System.out.println("두개의 주사위 합은 : " + sum + "입니다.");
	}

}
